{
  "name": "astroboy-router",
  "version": "2.2.1",
  "description": "A router plugin for astroboy.ts.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "ts:main": "./src/index.ts",
  "scripts": {
    "test": "mocha",
    "cover": "nyc mocha",
    "coverage": "npm run cover && nyc report --reporter=text-lcov | coveralls",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ws-node/astroboy-router.git"
  },
  "author": "mogician",
  "license": "MIT",
  "nyc": {
    "include": [
      "src/**/*.ts",
      "test/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "@types/koa": "^2.0.46",
    "@types/koa-router": "^7.0.31",
    "@types/node": "^10.3.2",
    "lodash": "^4.17.11",
    "reflect-metadata": "0.1.12",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@bigmogician/publisher": "^1.0.9",
    "@types/chai": "^4.1.6",
    "@types/lodash": "^4.14.116",
    "@types/mocha": "^5.2.5",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "source-map-support": "^0.5.9",
    "ts-node": "7.0.0",
    "typescript": "^3.0.3"
  }
}